{
  "presets": [
    "es2015",
    "react",
    "catalog/babel"
  ],
  "plugins": [
    "lodash",
    "syntax-dynamic-import",
    "syntax-object-rest-spread",
    "transform-object-rest-spread"
  ],
  "env": {
    "production": {
      "plugins": [
        [
          "transform-react-remove-prop-types",
          {
            "mode": "wrap"
          }
        ]
      ]
    }
  }
}
