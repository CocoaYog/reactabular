{
  "name": "reactabular",
  "version": "1.2.6-alpha.61e29539",
  "description": "Spectacular tables for React",
  "scripts": {
    "build:all": "npm run build:es5 && npm run build:es6",
    "build:es5": "rimraf ./dist-es5 && cross-env BABEL_ENV=es5 babel ./src --out-dir ./dist-es5",
    "build:es6": "rimraf ./dist-es6 && cross-env BABEL_ENV=es6 babel ./src --out-dir ./dist-es6",
    "prepublish": "in-publish && npm run build:all && cd ../.. && npm run dist:all && git commit --allow-empty -am \"Update dist\" || :",
    "postpublish": "cd ../.. && npm run gh-pages:build-and-deploy"
  },
  "files": [
    "dist-es5",
    "dist-es6",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "readmeFilename": "README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/reactabular/reactabular.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "table",
    "tables",
    "reactabular"
  ],
  "author": "Juho Vepsalainen <bebraw@gmail.com> (http://github.com/bebraw)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/reactabular/reactabular/issues"
  },
  "homepage": "http://reactabular.js.org/",
  "main": "dist-es5",
  "module": "dist-es6",
  "jsnext:main": "dist-es6",
  "dependencies": {
    "reactabular-highlight": "^1.2.6-alpha.61e29539",
    "reactabular-resizable": "^1.2.6-alpha.61e29539",
    "reactabular-resolve": "^1.2.6-alpha.61e29539",
    "reactabular-search": "^1.2.6-alpha.61e29539",
    "reactabular-search-columns": "^1.2.6-alpha.61e29539",
    "reactabular-search-field": "^1.2.6-alpha.61e29539",
    "reactabular-select": "^1.2.6-alpha.61e29539",
    "reactabular-sticky": "^1.2.6-alpha.61e29539",
    "reactabular-sort": "^1.2.6-alpha.61e29539",
    "reactabular-table": "^1.2.6-alpha.61e29539"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "cross-env": "^2.0.0",
    "in-publish": "^2.0.0",
    "rimraf": "^2.5.4"
  },
  "peerDependencies": {
    "lodash": ">= 3.0.0 < 5.0.0",
    "react": ">= 0.11.2 < 16.0.0"
  }
}
