{
  "name": "reactabular",
  "version": "1.0.2",
  "description": "Spectacular tables for React",
  "scripts": {
    "build": "rimraf ./dist-modules && babel ./src --out-dir ./dist-modules",
    "preversion": "npm run build && cd ../.. && npm run dist:all && git commit --allow-empty -am \"Update dist\"",
    "postpublish": "cd ../.. && npm run gh-pages:build-and-deploy"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/reactabular/reactabular.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "table",
    "tables",
    "reactabular"
  ],
  "author": "Juho Vepsalainen <bebraw@gmail.com> (http://github.com/bebraw)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/reactabular/reactabular/issues"
  },
  "homepage": "http://reactabular.js.org/",
  "main": "dist-modules",
  "jsnext:main": "src",
  "dependencies": {
    "reactabular-edit": "^1.0.2",
    "reactabular-highlight": "^1.0.2",
    "reactabular-resizable": "^1.0.2",
    "reactabular-resolve": "^1.0.2",
    "reactabular-search": "^1.0.2",
    "reactabular-sticky": "^1.0.2",
    "reactabular-sort": "^1.0.2",
    "reactabular-table": "^1.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "rimraf": "^2.5.4"
  },
  "peerDependencies": {
    "lodash": ">= 3.0.0 < 5.0.0",
    "react": ">= 0.11.2 < 16.0.0"
  }
}
