{
  "name": "reactabular",
  "version": "0.7.4",
  "description": "Spectacular tables for React.js",
  "scripts": {
    "start": "webpack-dev-server",
    "test": "BABEL_JEST_STAGE=1 jest && npm run lint",
    "gh-pages": "webpack",
    "deploy-gh-pages": "node ./lib/deploy-gh-pages.js",
    "dist": "webpack",
    "dist-min": "webpack",
    "dist-modules": "babel ./src --out-dir ./dist-modules",
    "lint": "eslint . --ext .js --ext .jsx",
    "preversion": "npm run test && npm run dist && npm run dist-min && npm run dist-modules && git commit --allow-empty -am \"Update dist\"",
    "postpublish": "npm run gh-pages && npm run deploy-gh-pages"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bebraw/reactabular.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "table",
    "tables"
  ],
  "author": "Juho Vepsalainen <bebraw@gmail.com> (http://github.com/bebraw)",
  "license": "MIT",
  "main": "dist-modules/index.js",
  "bugs": {
    "url": "https://github.com/bebraw/reactabular/issues"
  },
  "homepage": "https://bebraw.github.io/reactabular/",
  "dependencies": {
    "classnames": "^2.1.3",
    "lodash.sortbyorder": "^3.4.4"
  },
  "devDependencies": {
    "annogenerate": "^0.8.1",
    "annomath": "^0.4.1",
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "babel-eslint": "^4.1.3",
    "babel-jest": "^5.3.0",
    "babel-loader": "^5.3.2",
    "babel-plugin-react-transform": "^1.1.1",
    "css-loader": "^0.18.0",
    "eslint": "^1.5.0",
    "eslint-plugin-react": "^3.4.2",
    "file-loader": "^0.8.4",
    "gh-pages": "^0.4.0",
    "highlight.js": "^8.8.0",
    "html-loader": "^0.3.0",
    "html-webpack-plugin": "^1.6.1",
    "jest-cli": "^0.5.7",
    "lodash": "^3.10.1",
    "marked": "^0.3.5",
    "node-libs-browser": "^0.5.3",
    "plexus-form": "^0.1.3",
    "plexus-validate": "^0.0.4",
    "purecss": "^0.6.0",
    "react": "^0.13.3",
    "react-ghfork": "^0.3.2",
    "react-pagify": "^0.8.1",
    "react-skylight": "^0.2.0",
    "react-transform-hmr": "^1.0.1",
    "schema2object": "^0.4.0",
    "style-loader": "^0.12.4",
    "title-case": "^1.1.1",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.11.0",
    "webpack-merge": "^0.2.0"
  },
  "peerDependencies": {
    "lodash": ">= 3.5.0 < 4.0.0",
    "react": ">= 0.11.2 < 1.0.0"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testFileExtensions": ["js", "jsx"],
    "moduleFileExtensions": ["js", "jsx"],
    "unmockedModulePathPatterns": [
      "./node_modules/annotate",
      "./node_modules/lodash",
      "./node_modules/react",
      "./lib/editors"
    ]
  }
}
