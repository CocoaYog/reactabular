    "babel-plugin-react-transform": "^1.1.1",
    "chai": "^3.3.0",
    "classnames": "^2.2.0",
    "clean-webpack-plugin": "^0.1.3",
    "css-loader": "^0.18.0",
    "eslint": "^1.5.0",
    "eslint-plugin-react": "^3.4.2",
    "file-loader": "^0.8.4",
    "gh-pages": "^0.4.0",
    "highlight.js": "^8.8.0",
    "html-loader": "^0.3.0",
    "html-webpack-plugin": "^1.6.1",
    "isparta-instrumenter-loader": "^0.2.1",
    "karma": "^0.13.10",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^0.5.2",
